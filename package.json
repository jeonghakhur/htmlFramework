{
  "name": "wooribankmydata",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --parallel watch",
    "build": "npm-run-all --parallel css js html-list",
    "live-server": "live-server --port=5600",
    "css": "npm-run-all --parallel css-lint css-compile css-prefix css-minify",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss -o dist/css",
    "css-lint": "stylelint \"scss/**/*.scss\" --cache --cache-location .cache/.stylelintcache",
    "css-minify": "cleancss -O1 --format breakWith=lf --with-rebase --input-source-map --source-map-inline-sources --output dist/css/index.min.css dist/css/index.css",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "js": "npm-run-all --parallel js-lint js-compile",
    "js-compile": "rollup --environment BUNDLE:false --config build/rollup.config.js --sourcemap",
    "js-lint": "eslint \"js/**/*.js\" --cache --cache-location .cache/.eslintcache --report-unused-disable-directives",
    "html-list": "node build/html-list",
    "watch": "npm-run-all --parallel watch-*",
    "watch-html": "nodemon --watch html/ --ext html --exec \"npm-run-all html-list\"",
    "watch-js": "nodemon --watch js/ --ext js --exec \"npm-run-all js-compile\"",
    "watch-css": "nodemon --watch scss/ --ext scss --exec \"npm-run-all css-compile\"",
    "js-beautify": "node js-beautify"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "clean-css-cli": "^5.1.0",
    "core-js": "3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-config-xo": "^0.34.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-unicorn": "^28.0.2",
    "jquery": "^3.5.1",
    "js-beautify": "^1.14.0",
    "live-server": "^1.2.1",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.0",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "posthtml": "^0.16.4",
    "posthtml-attrs-sorter": "^1.1.0",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "stylelint": "^13.10.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0",
    "swiper": "^6.4.15"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
